# Database Configuration
DATABASE_URL=postgres://postgres:postgres@localhost:5432/agentic_browser?sslmode=disable
REDIS_URL=redis://localhost:6379

# Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRY=24h
REFRESH_TOKEN_EXPIRY=168h

# AI Services
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
AI_MODEL_PROVIDER=openai
AI_MODEL_NAME=gpt-4-turbo-preview

# Web3 Configuration
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/your-project-id
POLYGON_RPC_URL=https://polygon-mainnet.infura.io/v3/your-project-id
ARBITRUM_RPC_URL=https://arbitrum-mainnet.infura.io/v3/your-project-id
OPTIMISM_RPC_URL=https://optimism-mainnet.infura.io/v3/your-project-id
BSC_MAINNET_RPC_URL=https://bsc-dataseed.binance.org/
BSC_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545/
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your-project-id

# RPC Provider API Keys
INFURA_API_KEY=your-infura-api-key
ALCHEMY_API_KEY=your-alchemy-api-key
QUICKNODE_API_KEY=your-quicknode-api-key

# IPFS Configuration
IPFS_NODE_URL=http://localhost:5001
IPFS_GATEWAY=https://ipfs.io
IPFS_MAX_FILE_SIZE=10485760

# Web3 Features
WEB3_GAS_OPTIMIZATION=true
WEB3_HARDWARE_WALLETS=true
WEB3_ENS_RESOLUTION=true

# Browser Service
CHROME_HEADLESS=true
CHROME_DISABLE_GPU=true
CHROME_NO_SANDBOX=true
BROWSER_TIMEOUT=30s

# Observability
JAEGER_ENDPOINT=http://localhost:14268/api/traces
PROMETHEUS_ENDPOINT=http://localhost:9090
OTEL_SERVICE_NAME=agentic-browser
OTEL_EXPORTER_JAEGER_ENDPOINT=http://localhost:14268/api/traces

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST=20

# Security
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001
BCRYPT_COST=12

# Development
LOG_LEVEL=info
LOG_FORMAT=json
DEBUG=false

# Frontend Environment Variables
NEXT_PUBLIC_API_URL=http://localhost:8080
NEXT_PUBLIC_WS_URL=ws://localhost:8080
NEXT_PUBLIC_CHAIN_ID=1
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your-walletconnect-project-id
NEXT_PUBLIC_INFURA_KEY=your-infura-api-key
NEXT_PUBLIC_ALCHEMY_KEY=your-alchemy-api-key
NEXT_PUBLIC_QUICKNODE_KEY=your-quicknode-api-key
